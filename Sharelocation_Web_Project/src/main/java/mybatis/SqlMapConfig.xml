<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-config.dtd">
			
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	<typeAliases>
		<typeAlias alias="MySpace" type="space.model.SpaceBean"/>
		<typeAlias alias="MyReview" type="reviewBoard.model.ReviewBoardBean"/>
		<typeAlias alias="MyDetailSpace" type="detailspace.model.DetailSpaceBean"/>
		<typeAlias alias="MyMember" type="member.model.MemberBean"/>
		<typeAlias alias="MyReservation" type="reservation.model.ReservationBean"/>
		<typeAlias alias="MySpaceImage" type="space.model.SpaceImageBean"/>
		<typeAlias alias="MySpaceFacility" type="space.model.SpaceFacilityBean"/>
		<typeAlias alias="MyBalance" type="reservation.model.BalanceBean"/>
		<typeAlias alias="MyPackage" type="detailspace.model.PackagePriceBean"/>
		<typeAlias alias="MyFavorite" type="space.model.FavoriteBean"/>
		<typeAlias alias="MyAdvertise" type="space.model.AdvertiseBean"/>
		<typeAlias alias="MyNotice" type="noticeBoard.model.NoticeBoardBean"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/balance.xml"/>
		<mapper resource="mybatis/space.xml"/>
		<mapper resource="mybatis/detailSpace.xml"/>     
		<mapper resource="mybatis/reviewBoard.xml"/>     
		<mapper resource="mybatis/member.xml"/>
		<mapper resource="mybatis/reservation.xml"/>
		<mapper resource="mybatis/noticeBoard.xml"/>
	</mappers>
</configuration>